{
  "javaApp": {
    "path": "./java",
    "maxParallelTransforms": 10,
    "pollInterval": 100
  },
  "rendering": {
    "url": "http://127.0.0.1/faust-gen/transcript-generation.html"
  },
  "input": {
    "doc": {
      "metadata": "/home/tv/Faust/document/",
      "transcript": "/home/tv/Faust/transcript/",
      "imageTextLink": "/home/tv/Faust/image-text-links/",
      "facsimile": "/var/www/html/transcript/facsimile/"
    }
  },
  "output": {
    "jsonDoc": {
      "metadata": "./out2/document",
      "transcript": "./out2/transcript"
    },
    "svg": {
      "diplomatic": "./out2/svg/diplomatic",
      "facsimileOverlay": "./out2/svg/overlay"
    }
  },
  "errorLog": "error.log"
}
